{
  "enable": true,
  "name": "<name of setting e.g. cassandra, scylla, etc.>",
  "ipAddresses": ["<add ip>", "<add ip2>", "..."],
  "port": 9042,
  "secureConnectionBundle": "<link to '*.zip' file, downloaded from AstraDB web>",
  "username": "<add username>",
  "pwd": "<add pwd>",
  "localDC": "<local data center>",
  "connectionTimeout": "<timeout in seconds>",
  "requestTimeout": "<timeout in seconds>",
  "consistencyLevel": "<consistency level e.g. LOCAL_ONE, LOCAL_QUORUM, etc.>",

  "writeConsistencyLevel": "<consistency level e.g. LOCAL_ONE, LOCAL_QUORUM, etc.>",
  "batchSize": 200,
  "batchType": "<batch type, possible values UNLOGGED, LOGGED>",
  "table": "<add 'keyspace.table' in CQL>",
  "dryRun": false,
  "compaction": "<setting for compaction e.g. {'class':'SizeTieredCompactionStrategy'}>"
}